<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>

<BorderPane fx:id="mainPane" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.reconstruct.view.controller.RCBeamAnalysisDesignController">
   <center>
      <StackPane style="-fx-background-color: white; -fx-border-color: lightGray;">
         <BorderPane.margin>
            <Insets bottom="2.5" left="5.0" right="2.5" top="5.0" />
         </BorderPane.margin>
         <children>
            <AreaChart fx:id="areaChart" animated="false" createSymbols="false" legendSide="TOP" style="-fx-background-color: white; -fx-padding: 30;" stylesheets="@css/chart-beam-style.css">
              <xAxis>
                <NumberAxis side="BOTTOM" style="-fx-translate-y: 30;" fx:id="xAxis" />
              </xAxis>
              <yAxis>
                <NumberAxis fx:id="yAxis" prefWidth="45.0" side="LEFT" style="-fx-translate-x: -30;" stylesheets="@css/chart-beam-style.css">
                     <opaqueInsets>
                        <Insets left="77.0" />
                     </opaqueInsets>
                  </NumberAxis>
              </yAxis>
            </AreaChart>
            <StackPane fx:id="elementsStackPane" StackPane.alignment="CENTER">
               <StackPane.margin>
                  <Insets bottom="65.0" left="36.0" right="40.0" top="40.0" />
               </StackPane.margin>
               <children>
                  <StackPane fx:id="rollerPane" maxHeight="40.0" maxWidth="40.0" minHeight="40.0" minWidth="40.0" prefHeight="40.0" prefWidth="40.0" StackPane.alignment="CENTER_LEFT">
                     <StackPane.margin>
                        <Insets left="30.0" top="40.0" />
                     </StackPane.margin>
                     <children>
                        <Circle fill="WHITE" radius="12.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="1.5" StackPane.alignment="TOP_CENTER" />
                     </children>
                  </StackPane>
                  <StackPane fx:id="pinnedPane" maxHeight="40.0" maxWidth="40.0" minHeight="40.0" minWidth="40.0" prefHeight="40.0" prefWidth="40.0" StackPane.alignment="CENTER_LEFT">
                     <StackPane.margin>
                        <Insets left="30.0" top="40.0" />
                     </StackPane.margin>
                     <children>
                        <Polygon fill="WHITE" stroke="BLACK" strokeLineCap="BUTT" strokeWidth="1.5" StackPane.alignment="TOP_CENTER">
                          <points>
                            <Double fx:value="-10.5" />
                            <Double fx:value="10.5" />
                            <Double fx:value="10.5" />
                            <Double fx:value="10.5" />
                            <Double fx:value="0.0" />
                            <Double fx:value="-10.5" />
                          </points>
                        </Polygon>
                     </children>
                  </StackPane>
               </children>
            </StackPane>
         </children>
      </StackPane>
   </center>
   <bottom>
      <VBox spacing="15.0" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </BorderPane.margin>
         <children>
            <ScrollPane fx:id="scrollPaneWorkSpace" fitToWidth="true" prefHeight="300.0">
               <padding>
                  <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
               </padding>
               <content>
                  <VBox>
                     <children>
                        <TextField />
                     </children>
                  </VBox>
               </content>
            </ScrollPane>
            <Separator prefHeight="0.0" prefWidth="200.0">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Separator>
            <HBox alignment="CENTER_RIGHT" spacing="10.0">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <children>
                <Button fx:id="generateButton" mnemonicParsing="false" prefWidth="75.0" text="Generate" />
                <Button fx:id="cancelButton" cancelButton="true" layoutX="595.0" layoutY="10.0" mnemonicParsing="false" prefWidth="75.0" text="Cancel" />
               </children>
            </HBox>
         </children>
      </VBox>
   </bottom>
   <right>
      <VBox prefWidth="150.0" spacing="15.0" BorderPane.alignment="TOP_LEFT">
         <children>
            <VBox VBox.vgrow="NEVER">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
               <children>
                  <HBox alignment="CENTER_LEFT" prefHeight="20.0" spacing="5.0">
                     <children>
                        <Label style="-fx-text-fill: SteelBlue;" text="View" />
                        <Separator HBox.hgrow="ALWAYS" />
                     </children>
                  </HBox>
                  <VBox spacing="5.0" VBox.vgrow="NEVER">
                     <children>
                        <CheckBox mnemonicParsing="false" text="Loading">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                        </CheckBox>
                        <CheckBox mnemonicParsing="false" text="Moment(kNm)" />
                        <CheckBox mnemonicParsing="false" text="Sheer(kN)" />
                     </children>
                     <VBox.margin>
                        <Insets left="15.0" />
                     </VBox.margin>
                  </VBox>
               </children>
            </VBox>
            <VBox spacing="5.0">
               <children>
                  <HBox alignment="CENTER_LEFT" prefHeight="20.0" prefWidth="200.0" spacing="5.0">
                     <children>
                        <Label style="-fx-text-fill: SteelBlue;" text="Model" />
                        <Separator HBox.hgrow="ALWAYS" />
                     </children>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <VBox.margin>
                        <Insets bottom="5.0" />
                     </VBox.margin>
                  </HBox>
                  <Button alignment="TOP_CENTER" mnemonicParsing="false" onAction="#onGeometryButtonAction" prefWidth="200.0" text="Geometry" VBox.vgrow="ALWAYS">
                     <VBox.margin>
                        <Insets left="15.0" />
                     </VBox.margin>
                  </Button>
                  <Button alignment="TOP_CENTER" mnemonicParsing="false" onAction="#onLoadingButtonAction" prefWidth="200.0" text="Loading" VBox.vgrow="ALWAYS">
                     <VBox.margin>
                        <Insets left="15.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
   </right>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</BorderPane>
